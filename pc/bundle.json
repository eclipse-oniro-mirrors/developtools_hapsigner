{
    "name": "@ohos/hapsigner",
    "version": "3.1",
    "description": "hap包签名工具，支持.hsp、.hqf、.hap和.app等文件签名",
    "homePage": "https://gitee.com/openharmony",
    "license": "Apache License 2.0",
    "repository": "https://gitee.com/openharmony/developtools_hapsigner",
    "domain": "os",
    "language": "",
    "publishAs": "code-segment",
    "private": false,
    "scripts": {},
    "tags": [
        "developtools"
    ],
    "keywords": [
        "hapsigner"
    ],
    "envs": {},
    "dirs": {},
    "segment": {
        "destPath": "developtools/hapsigner"
    },
    "component": {
        "name": "hapsigner",
        "subsystem": "developtools",
        "syscap": [],
        "features": [],
        "adapted_system_type": [ "standard" ],
        "rom": "0KB",
        "ram": "0KB",
        "deps": {
            "components": [
                "c_utils",
                "hilog",
                "json"
            ],
            "third_party": [
                "bzip2",
                "openssl",
                "jsoncpp",
                "zlib"
            ]
        },
        "build": {
            "sub_component": [
                "//developtools/hapsigner/pc/services:hap-sign-tool"
            ],
            "inner_kits": [],
            "test": [
                "//developtools/hapsigner/pc/test/unittest:hapsigntool_pc_unittest",
                "//developtools/hapsigner/pc/test/fuzztest:hapsigntool_pc_fuzztest"
            ]
        }
    }
}
